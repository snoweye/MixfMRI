citHeader("To cite MixfMRI in a publication use:")

bibentry(
  bibtype = "Article",
  title = "A practical model-based segmentation approach for improved activation detection in single-subject functional magnetic resonance imaging studies",
  author = c("Wei-Chen Chen",
             "Ranjan Maitra"),
  year = "2023",
  journal = "Human Brain Mapping",
  url = "https://doi.org/10.1002/hbm.26425",
  pages = "1-27",
  textVersion = "Chen, W.-C. and Maitra, R. (2023). A practical model-based segmentation approach for improved activation detection in single-subject functional magnetic resonance imaging studies. Human Brain Mapping, 1-27",
  key = "ChenMaitra2023HBM"
)

bibentry(
  bibtype = "Misc",
  title = "{MixfMRI}: {fMRI} Clustering Analysis",
  author = c("Wei-Chen Chen",
             "Ranjan Maitra"),
  year = "2018",
  note = "{R} Package, URL https://cran.r-project.org/package=MixfMRI",
  textVersion = "Chen, W.-C. and Maitra, R. (2018) MixfMRI: fMRI Clustering Analysis. R Package, URL https://cran.r-project.org/package=MixfMRI",
  key = "Chen2018MixfMRIpackage"
)

bibentry(
  bibtype = "Manual",
  title = "A Quick Guide to the {MixfMRI} Package",
  author = c("Wei-Chen Chen",
             "Ranjan Maitra"),
  year = "2018",
  note = "{R} Vignette",
  textVersion = "Chen, W.-C. and Maitra, R. (2018) A Quick Guide to the MixfMRI Package. R Vignette, URL https://cran.r-project.org/package=MixfMRI",
  key = "Chen2018MixfMRIvignette"
)
